#include "../spinOperators1D.hpp"

int main(void) {
  SpinZHalfOp Sz1(1);
  std::cout << "Sz1 = " << Sz1.toString() << std::endl;
  SpinHalfMonomial mn1(Sz1);
  mn1 *= Sz1;
  std::cout << "mn1 = " << mn1.toString() << std::endl;
  std::cout << "size(mn1) = " << mn1.getSize() << std::endl;
  return EXIT_SUCCESS;
}
